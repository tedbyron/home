{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.3.0"
  },
  "scripts": {
    "postinstall": "svelte-kit sync",
    "dev": "svelte-kit dev",
    "build": "pnpm check && pnpm lint && svelte-kit build",
    "serve": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier -c --ignore-path=.gitignore --plugin-search-dir=. . '!pnpm-lock.yaml' && eslint --ignore-path=.gitignore .",
    "format": "prettier -w --ignore-path=.gitignore --plugin-search-dir=. . '!pnpm-lock.yaml'"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.2",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "autoprefixer": "10.4.5",
    "cssnano": "5.1.12",
    "cssnano-preset-advanced": "5.3.8",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.3",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "svelte": "3.48.0",
    "svelte-check": "2.7.2",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.1.4",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "unplugin-auto-import": "0.9.1",
    "wrangler": "2.0.15"
  }
}
