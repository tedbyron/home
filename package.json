{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@7.4.1",
  "engines": {
    "node": ">=16",
    "pnpm": "7.4.1"
  },
  "scripts": {
    "postinstall": "svelte-kit sync",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path=.gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "ns": "wrangler kv:namespace",
    "key": "wrangler kv:key",
    "bulk": "wrangler kv:bulk"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.0",
    "@iconify-json/tabler": "1.1.18",
    "@sveltejs/adapter-cloudflare": "1.0.0-next.24",
    "@sveltejs/kit": "next",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.2",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "autoprefixer": "10.4.5",
    "cssnano": "5.1.12",
    "cssnano-preset-advanced": "5.3.8",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.1.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.3",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "svelte": "3.48.0",
    "svelte-check": "2.8.0",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.1.4",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "unplugin-icons": "0.14.7",
    "wrangler": "2.0.15"
  }
}
