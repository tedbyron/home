{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "outputs": ["tsconfig.tsbuildinfo"]
    },
    "format": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["lint", "^build"],
      "outputs": [".cache/**", "public/build/**", "build/**", "target/**"]
    }
  }
}
